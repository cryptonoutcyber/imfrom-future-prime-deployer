{
  "name": "omega-prime-deployer",
  "version": "1.2.0",
  "description": "Scripts and a VS Code extension to deploy the Omega Prime token (IMPULSE / Î©AGENT) on Solana.",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "dev:check": "ts-node src/utils/checkEnv.ts",
    "mainnet:create-mint": "ts-node src/createMint.ts",
    "mainnet:mint-initial": "ts-node src/mintInitial.ts",
    "mainnet:set-metadata": "ts-node src/setMetadata.ts",
    "mainnet:lock": "ts-node src/lockAuthorities.ts",
    "mainnet:withdraw-earnings": "ts-node src/withdraw_earnings.ts",
    "mainnet:rollback": "ts-node src/rollback.ts",
    "mainnet:copilot": "ts-node grok-copilot.ts",
    "mainnet:all": "npm run dev:check && npm run mainnet:create-mint && npm run mainnet:mint-initial && npm run mainnet:set-metadata && npm run mainnet:lock",
    "mainnet:bot-orchestrate": "ts-node src/botOrchestrator.ts",
    "mainnet:reannounce-controller": "ts-node src/reannounceController.ts",
    "mainnet:verify-bots": "ts-node src/verifyBotBalances.ts",
    "scan:token-program": "ts-node src/fetchAllTokenProgramAccounts.ts",
    "imperial:deploy": "ts-node imperial-empire.ts",
    "imperial:trading": "ts-node imperial-trading-agent.ts",
    "imperial:python": "python3 deploy_imperial_empire.py",
    "imperial:transfer": "node imperial-ownership-transfer.js",
    "imperial:full": "npm run imperial:deploy && npm run imperial:trading",
    "audit": "node run-audit.js",
    "audit:contracts": "node comprehensive-omega-audit.js",
    "audit:github": "node github-account-scanner.js",
    "audit:addresses": "node address-audit.js",
    "scan:contracts": "node contract-address-transaction-scanner.js",
    "scan:contracts:offline": "node contract-scanner-offline.js",
    "build": "tsc",
    "build:rust": "cargo build --manifest-path pentacle/Cargo.toml",
    "build:all": "npm run build && npm run build:rust",
    "lint": "eslint src --ext ts",
    "test": "npm run build && node ./out/test/runTest.js"
  },
  "dependencies": {
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@metaplex-foundation/umi": "^1.4.1",
    "@metaplex-foundation/umi-bundle-defaults": "^1.4.1",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "bs58": "^6.0.0",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@types/node": "^22.18.5",
    "@types/vscode": "^1.101.0",
    "@vscode/extension-telemetry": "^0.9.9",
    "eslint": "^8.50.0",
    "hardhat": "^3.0.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "engines": {
    "vscode": "^1.101.0"
  },
  "keywords": [
    "solana",
    "spl-token",
    "omega-prime",
    "token-deployment",
    "relayer",
    "blockchain",
    "web3"
  ],
  "author": "AutomataLabs",
  "license": "MIT"
}
